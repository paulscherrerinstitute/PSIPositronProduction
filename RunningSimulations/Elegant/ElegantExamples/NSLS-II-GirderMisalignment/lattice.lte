DM2B: DRIF,L=0.67
DM2C: DRIF,L=0.20
DPW : DRIF,L=0.20
DSCH: DRIF,L=0.1
GHS1 : MULT,group="girder1"
GHE1 : MULT,group="girder1"
GHS2 : MULT,group="girder1"
GHE2 : MULT,group="girder1"
GLS1 : MULT, group="girder3"
GLE1 : MULT, group="girder3"
